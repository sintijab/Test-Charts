{
  "parser": "babel-eslint",
  "rules": {
    "max-len": [1, 90, 2, {ignoreComments: true}],
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
    },
  "extends": ["react", "airbnb", "airbnb/hooks"],
  "env": {
    "browser": true,
    "node": true
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
